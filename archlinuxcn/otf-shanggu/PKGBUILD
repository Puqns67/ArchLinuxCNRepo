    install -Dm644 -t "$pkgdir/usr/share/licenses/$pkgname" ShangguSerif/LICENSE.txt
}
package_otf-shanggu-serif-hw() {
    install -Dm644 -t "$pkgdir/usr/share/fonts/OTF/$pkgname" ShangguSerif/ShangguSerifHW-*.otf
    install -Dm644 -t "$pkgdir/usr/share/licenses/$pkgname" ShangguSerif/LICENSE.txt
}
